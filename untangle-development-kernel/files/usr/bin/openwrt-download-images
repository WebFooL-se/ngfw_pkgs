#!/bin/sh

# Realview
#wget https://archive.openwrt.org/snapshots/trunk/realview/generic/openwrt-realview-sdcard.img
#wget https://archive.openwrt.org/snapshots/trunk/realview/generic/openwrt-realview-vmlinux.elf

# x86_64 images
wget https://archive.openwrt.org/snapshots/trunk/x86/64/openwrt-x86-64-combined-ext4.img.gz
gzip -d openwrt-x86-64-combined-ext4.img.gz
wget https://archive.openwrt.org/snapshots/trunk/x86/64/openwrt-x86-64-vmlinuz

# create a disk data image
qemu-img create -f qcow2 data.qcow2 2G
